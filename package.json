{
  "name": "sakura-node-3",
  "version": "3.4.11",
  "description": "Helpful startup framework for building RESTful service with PostgreSQL, MySQL",
  "main": "./lib/js/index.js",
  "types": "./lib/definitions/index.d.ts",
  "scripts": {
    "pretest": "rm -rf ./lib; gulp ts; sh ./bin/tslint.sh;",
    "test": "export NODE_ENV=test; mocha  './lib/js/src/test/**/*.js'   --recursive; export NODE_ENV=;",
    "posttest": "rm -rf ./lib",
    "prepublish": "rm -rf ./lib; gulp ts;",
    "postpublish": "rm -rf ./lib; gulp ts; node ./lib/js/src/tools/sendchangelog.js; rm -rf ./lib;",
    "lint": "sh bin/tslint.sh",
    "test-mysql": "sh bin/test-mysql.sh",
    "sample-mysql": "rm -rf ./lib; gulp ts; node ./lib/js/src/example/mysql/dbclient.js;",
    "pretypedoc": "mkdir docs",
    "typedoc": "./node_modules/typedoc/bin/typedoc --ignoreCompilerErrors --out ./docs --exclude test --excludePrivate --hideGenerator  --mode modules --tsconfig ./tsconfig.json ./src/",
    "posttypedoc": "touch docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DaYeSquad/sakura-node-ts.git"
  },
  "publishConfig": {
    "registry": "http://npmjs.gagogroup.cn"
  },
  "author": "Frank Lin",
  "license": "MIT",
  "dependencies": {
    "@types/mysql": "0.0.33",
    "@types/pg": "^6.1.33",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "moment": "^2.17.1",
    "mysql": "^2.13.0",
    "pg": "^6.1.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/connect-timeout": "0.0.31",
    "@types/express": "^4.0.33",
    "@types/mocha": "^2.2.32",
    "@types/moment": "^2.13.0",
    "@types/node": "^6.0.45",
    "@types/showdown": "^1.4.31",
    "@types/sinon": "^1.16.31",
    "@types/superagent": "^2.0.34",
    "@types/supertest": "^1.1.31",
    "@types/uuid": "^3.4.3",
    "@types/winston": "0.0.28",
    "chai": "^3.5.0",
    "child_process": "^1.0.2",
    "gago-cloud-service": "1.2.3",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^3.0.2",
    "merge2": "^1.0.2",
    "mocha": "^3.0.2",
    "run-sequence": "^1.2.2",
    "showdown": "^1.5.2",
    "sinon": "^1.17.6",
    "supertest": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-sakura-contrib": "^1.0.6",
    "typedoc": "^0.10.0",
    "typescript": "^2.0.3"
  }
}
